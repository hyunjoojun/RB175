<h2>Details for <%= @name %></h2>

<h4>Email address:</h4>
<p><%= @email %></p>

<h4>Interests:</h4>
<p><%= @interests.join(", ") %></p>

<p>Other users:</p>
  <ul>
    <% @users.keys.each do |user| %>
    <% next if user == @name %>
      <li>
        <a href="/<%= user %>">
          <%= user %>
        </a>
      </li>
    <% end %>
  </ul>
